{
    "extends": ["airbnb-base/legacy", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "func-names":"off",
        "no-restricted-globals":"off"
    },
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "env": {
        "jquery": true
    }
}